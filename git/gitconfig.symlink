	repo = /Users/pe8er/Developer/horizon-mini
	repo = /Users/pe8er/Developer/miryoku_zmk
	repo = /Users/pe8er/Developer/pe8er.net
	repo = /Users/pe8er/Developer/urchin
	repo = /Users/pe8er/Developer/zmk-config-corne
	repo = /Users/pe8er/Developer/zmk-config-horizon-mini
	repo = /Users/pe8er/Developer/znap
