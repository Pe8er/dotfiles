[user]
	email = pgajos@icloud.com
	name = Piotr Gajos
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
	b = 'branch'
	cam = "commit -am"
	cb='copy-branch-name'
	co = checkout
	d=diff
	p='push'
	plpr='pull --prune'
	xlog="log --graph --pretty=format:'%Cred%h%Creset %an: %s - %Creset %C(yellow)%d%Creset %Cgreen(%cr)%Creset' --abbrev-commit --date=relative"
  c = commit
  s = status
[pull]
	rebase = true
[maintenance]
	repo = /Users/pe8er/Developer/Code Brown
	repo = /Users/pe8er/Developer/chocV
	repo = /Users/pe8er/Developer/crkbd
